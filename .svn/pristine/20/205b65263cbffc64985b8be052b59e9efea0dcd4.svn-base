CC=gcc
FLAGS=-g -pedantic -Wall -std=gnu99
LIB_STRING_MAP_LIB=-L/local/courses/csse2310/lib -lstringmap
A4_HEADER=-I/local/courses/csse2310/include
A4_LIB=-L/local/courses/csse2310/lib -lcsse2310a4

all: client server

client: client.c
	$(CC) $(FLAGS) $(A4_HEADER) $(A4_LIB) -o psclient client.c

server: server.c
	$(CC) $(FLAGS) $(A4_HEADER) $(A4_LIB) -o psserver server.c

clean:
	rm psclient
	rm psserver



